                        <Rectangle x:Name="PART_Track" Margin="1"/>
                        <Decorator x:Name="PART_Indicator" HorizontalAlignment="Left" Margin="0">
                            <Border CornerRadius="2" BorderThickness="1" BorderBrush="#FFFB6413">
