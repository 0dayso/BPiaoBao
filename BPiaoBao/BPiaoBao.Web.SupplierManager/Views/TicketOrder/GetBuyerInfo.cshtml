@model BPiaoBao.AppServices.ConsoContracts.SystemSetting.DataObjects.ResponseDetailBuyer


    <table class="GBI_table" width="100%">
        <tr class="GBI_table_tr">
            <td width="70">商户号:</td>
            <td>@Model.Code</td>
        </tr>
        <tr>
            <td>商户名称:</td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>座机号码:</td>
            <td>@Model.Plane</td>
        </tr>
        <tr>
            <td>联系人:</td>
            <td>@Model.ContactWay.Contact</td>
        </tr>
        <tr>
            <td>联系电话:</td>
            <td>@Model.ContactWay.Tel</td>
        </tr>
        <tr>
            <td>联系地址:</td>
            <td>@(Model.ContactWay.Province + Model.ContactWay.City + Model.ContactWay.Address)</td>
        </tr>
    </table>

<style>
    .GBI_table td {
        line-height:32px; padding-left:10px; border-bottom:1px dashed #eee;
    }
    .GBI_table tr.GBI_table_tr td {
        padding-top:5px;
    }
</style>
