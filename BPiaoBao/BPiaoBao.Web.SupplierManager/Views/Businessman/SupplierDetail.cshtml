@model BPiaoBao.AppServices.ConsoContracts.SystemSetting.DataObjects.SupplierDataObject
<div style=" margin-left:40px; margin-top:15px; width:270px;">
    <table border="0" cellpadding="0" cellspacing="0" class="gongyingshang">
        <tr>
            <td colspan="2">
                <h3>商户信息</h3>
            </td>
        </tr>
        <tr>
            <td width="100">商户号：</td>
            <td>@Model.Code</td>
        </tr>
        <tr>
            <td>商户名称：</td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>本地费率：</td>
            <td>@Model.SupRate</td>
        </tr>
        <tr>
            <td>异地费率：</td>
            <td>@Model.SupRemoteRate</td>
        </tr>
    </table>

    <table border="0" cellpadding="0" cellspacing="0" class="gongyingshang">
        <tr>
            <td colspan="2" style="padding-top: 20px;">
                <h3>基本信息</h3>
            </td>
        </tr>
        <tr>
            <td width="100">联系人：</td>
            <td>@Model.ContactWay.Contact</td>
        </tr>
        <tr>
            <td>联系电话：</td>
            <td>@Model.ContactWay.Tel</td>
        </tr>
        <tr>
            <td>联系地址：</td>
            <td>
                @(Model.ContactWay.Province + Model.ContactWay.City + Model.ContactWay.Address)
            </td>
        </tr>

        <tr>
            <td>客户经理：</td>
            <td>@Model.ContactName</td>
        </tr>
        <tr>
            <td>客户经理电话：</td>
            <td>@Model.Phone</td>
        </tr>
    </table>

    <table border="0" cellspacing="0" cellpadding="0" class="gongyingshang">
        <tr>
            <td colspan="6">
                <h3 style="width: 100px; float: left; display: block; margin-top: 22px; margin-bottom: 15px;">配置信息</h3>
            </td>
        </tr>
        @foreach (var item in Model.Pids)
        {
            <tr class="trbgcolor">
                <td>IP：</td>
                <td>@item.IP</td>
                <td>端口：</td>
                <td>@item.Port</td>
                <td>Office号：</td>
                <td>@item.Office</td>
            </tr>
        }
    </table>

    <table border="0" cellspacing="0" cellpadding="0" class="gongyingshang">
        <tr>
            <td colspan="8">
                <h3 style="width: 150px; float: left; display: block; margin-top: 22px; margin-bottom: 15px;">航空公司使用Office</h3>

            </td>
        </tr>
        @foreach (var item in Model.CarrierSettings)
        {
            <tr class="trbgcolor">
                <td>航空公司：</td>
                <td>@item.CarrayCode</td>
                <td>预定编码Office：</td>
                <td>@item.YDOffice</td>
                <td>出票Office：</td>
                <td>@item.CPOffice</td>
                <td>打票机号：</td>
                <td>@item.PrintNo</td>
            </tr>
        }

    </table>

</div>


